{
  "name": "serverless-boilerplate-ts",
  "version": "1.0.0",
  "description": "Serverless Boilerplate TS",
  "scripts": {
    "db:generate": "npx sequelize-cli model:generate --name",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:all": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "deploy:all": "serverless deploy",
    "start:dev": "nodemon --exec \"serverless offline start\"",
    "test:coverage": "NODE_ENV=test & jest -i --silent --coverage",
    "test:local": "jest -i --coverage",
    
    "build": "tsc",
    "offline": "npm run build && serverless offline",
    "deploy": "npm run build && serverless deploy"
  },
  "keywords": [],
  "author": "Denmark",
  "license": "MIT",
  "dependencies": {
    "http-status": "^1.5.3",
    "joi": "^17.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.5.0",
    "serverless": "^3.24.1",
    "serverless-layers": "^2.6.1",
    "serverless-offline": "^11.6.0",
    "ts-node": "^10.9.2",
    "typescript": "4.9"
  }
}
